namespace _15
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string number = Console.ReadLine();
            int num1 = 0, num2 = 0;
            for (int i = 0; i < 6; i += 2) { num1 += int.Parse(number[i].ToString()); }
            for (int i = 1; i < 6; i += 2) { num2 += int.Parse(number[i].ToString()); }
            Console.WriteLine(num1 == num2);
        }
    }
}
